{
  "name": "@yondav/react-app-webpack-twin",
  "version": "0.0.0",
  "description": "TypeScript + React + Webpack + Tailwind + Twin Macro",
  "scripts": {
    "build": "webpack --mode=production",
    "commit": "node ./node_modules/cz-customizable/standalone.js",
    "dev": "webpack serve --mode=development",
    "lint:check": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:staged": "lint-staged --config lint-staged.js",
    "prepare": "husky install",
    "serve": "npx serve dist"
  },
  "dependencies": {
    "@types/js-cookie": "^3.0.2",
    "aws-sdk": "^2.1271.0",
    "axios": "^1.1.3",
    "firebase": "^9.15.0",
    "framer-motion": "^7.6.15",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.39.3",
    "react-icons": "^4.7.1",
    "react-images-uploading": "^3.1.7",
    "react-router-dom": "^6.4.2",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/react": "^18.0.18",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-plugin-twin": "^1.0.2",
    "commitlint": "^17.1.2",
    "commitlint-config-monorepo": "^2.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "cz-customizable": "^7.0.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "file-loader": "^6.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.8.0",
    "tailwindcss": "^3.2.0",
    "terser-webpack-plugin": "^5.3.6",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "twin.macro": "^2.8.2",
    "typescript": "^4.8.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "sh hooks/pre-commit",
      "commit-msg": "sh hooks/commit-msg"
    }
  }
}
